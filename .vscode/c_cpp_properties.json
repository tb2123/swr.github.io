"latex-workshop.latex.tools": [
    {
     "name": "latexmk",
     "command": "latexmk",
     "args": [
      "-synctex=1",
      "-interaction=nonstopmode",
      "-file-line-error",
      "-pdf",
      "-outdir=%OUTDIR%",
      "%DOC%"
     ],
     "env": {}
    },
    {
     "name": "xelatex",
     "command": "xelatex",
